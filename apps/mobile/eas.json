{
    "cli": {
        "version": ">= 3.0.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_API_URL": "https://pantheon.vercel.app/api"
            }
        },
        "production": {
            "node": "18.x",
            "env": {
                "EXPO_PUBLIC_API_URL": "https://pantheon.vercel.app/api",
                "NODE_ENV": "production"
            }
        }
    },
    "submit": {
        "production": {}
    }
}